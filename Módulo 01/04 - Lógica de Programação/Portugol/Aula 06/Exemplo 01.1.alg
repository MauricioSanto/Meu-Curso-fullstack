Algoritmo "Vetor - Conjunto de Dados - Tipos de dados"

Var listaNumeros: vetor[0..0] de inteiro

Inicio

    listaNumeros[0] <- 10
    listaNumeros[1] <- "oi"
    listaNumeros[2] <- falso
    listaNumeros[3] <- 40

    escreval (listaNumeros)

    escreval(listaNumeros[0] + 1)
    escreval(listaNumeros[1] + 1)

Fimalgoritmo